{
  "name": "faridlab/laravel-search-query",
  "description": "Search Query - Laravel package for filtering queries based on url query string parameters",
  "keywords": [
    "query",
    "query-string",
    "querystring",
    "filter",
    "filter-querystring",
    "filter-query-string"
  ],
  "homepage": "https://github.com/faridlab/laravel-search-query",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Farid Hidayat",
      "email": "e.faridhidayat@gmail.com",
      "role": "Senior Software Engineer"
    }
  ],
  "require": {
    "php": ">=7.1",
    "laravel/framework": ">=6.20.14"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0"
  },
  "autoload": {
    "files": [
      "helpers/helpers.php"
    ],
    "psr-4": {
      "SearchQuery\\FilterQueryString\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "SearchQuery\\FilterQueryString\\Models\\": "models/",
      "SearchQuery\\FilterQueryString\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  }
}
